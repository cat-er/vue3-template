{
  "name": "vue-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --config ./config/vite.config.dev.ts",
    "pro": "vite --config ./config/vite.config.pro.ts",
    "build:dev": "run-s type-check build-only:dev",
    "build:pro": "run-s type-check:strict build-only:pro",
    "build-only:dev": "vite build --config ./config/vite.config.dev.ts",
    "build-only:pro": "vite build --config ./config/vite.config.pro.ts",
    "type-check": "vue-tsc --noEmit --skipLibCheck",
    "type-check:strict": "vue-tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@types/nprogress": "^0.2.3",
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "ant-design-vue": "^4.2.3",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "query-string": "^9.0.0",
    "uuid": "^10.0.0",
    "vue": "^3.4.29",
    "vue-draggable-plus": "^0.5.2",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.14.5",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "less": "^4.2.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "unplugin-vue-components": "^0.27.2",
    "vite": "^5.3.1",
    "vite-plugin-autogeneration-import-file": "^3.0.0",
    "vue-tsc": "^2.0.21"
  }
}
